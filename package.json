{
  "name": "perfin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.app.json && vite build",
    "preview": "vite preview",
    "cftypes": "wrangler types server/cloudflare.d.ts && prettier -w server/cloudflare.d.ts",
    "migration:generate": "drizzle-kit generate",
    "migration:create": "wrangler d1 migrations create perfin-staging $(pnpm dlx human-id a l 0 _)",
    "migration:apply": "wrangler d1 migrations apply perfin-staging --local",
    "migration:apply-staging": "wrangler d1 migrations apply perfin-staging --remote",
    "runsql": "wrangler d1 execute perfin-staging --local --command",
    "runsqlfile": "wrangler d1 execute perfin-staging --local --file",
    "deploy:staging": "wrangler d1 migrations apply perfin-staging --remote --env staging & wrangler deploy --keep-vars --env staging"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-router": "^1.144.0",
    "@trpc/client": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@trpc/tanstack-react-query": "^11.8.1",
    "@types/node": "^25.0.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "1.0.0-beta.4-4605abe",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "ua-parser-js": "^2.0.7",
    "zod": "^4.3.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.17.1",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/router-plugin": "^1.145.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "daisyui": "^5.5.14",
    "drizzle-kit": "1.0.0-beta.4-4605abe",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "nanoid": "^5.1.5",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "wrangler": "^4.54.0"
  },
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501"
}
